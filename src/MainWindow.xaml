<Window
    x:Class="TingenCommander.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TingenCommander"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="800"
    Height="450"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <Grid>
        <Label
            x:Name="lblTingenDevelopmentVersion"
            Margin="92,68,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Tingen development" />
        <TextBox
            x:Name="txbxTingenDevelopmentVersion"
            Width="120"
            Margin="244,81,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Text="---"
            TextWrapping="Wrap" />



        <Label
            x:Name="lblTingenUatVersion"
            Margin="86,99,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Content="Tingen UAT" />
        <TextBox
            x:Name="txbxTingenUatVersion"
            Width="120"
            Margin="162,103,0,0"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Text="---"
            TextWrapping="Wrap" />



    </Grid>
</Window>
